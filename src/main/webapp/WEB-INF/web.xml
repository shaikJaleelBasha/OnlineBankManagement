<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>teluskojsp</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>

	</welcome-file-list>


	<servlet>
		<servlet-name>RegistrationServlet</servlet-name>
		<servlet-class>com.controller.RegistrationServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RegistrationServlet</servlet-name>
		<url-pattern>/submit_registration</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.controller.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>transactionHistory</servlet-name>
		<servlet-class>com.controller.TransactionHistoryServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>transactionHistory</servlet-name>
		<url-pattern>/transactionHistory</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.controller.LogoutPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Transaction</servlet-name>
		<servlet-class>com.controller.TransactionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Transaction</servlet-name>
		<url-pattern>/transaction</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>RegisterApp</servlet-name>
		<servlet-class>com.controller.RegistrationLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterApp</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CheckBalanceServlet</servlet-name>
		<servlet-class>com.controller.CheckBalanceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckBalanceServlet</servlet-name>
		<url-pattern>/checkBalance</url-pattern>
	</servlet-mapping>



</web-app>